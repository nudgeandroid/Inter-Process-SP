<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.qingtui.multiprocesssp">

    <application
            android:name=".TestApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <service
                android:name=".MyIntentService"
                android:process=":core"
                android:exported="true">
        </service>

        <activity
                android:name=".Main2Activity"
                android:process=":core">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

<!--        <receiver
                android:name=".SPBroadcast"
                android:exported="true"
                android:process=":core">
            <intent-filter>
                <action android:name="im.qingtui.android.multi.provider.sp.broadcast"/>
            </intent-filter>
        </receiver>-->

        <meta-data
                android:name="SP_Provider_Author"
                android:value="im.qingtui.android.multi.provider.sp"/>
        <provider
                android:name=".lib.SharedPreferencesProvider"
                android:authorities="im.qingtui.android.multi.provider.sp"
                android:exported="false"/>
    </application>

</manifest>